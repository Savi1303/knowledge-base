{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/pages/function.js", "src/app/pages/vapi/Config/anon.jsx", "src/app/pages/vapi/vapiUpload.jsx", "src/pages/vapi/vapi-call.tsx", "src/pages/openai/Config/openai.jsx", "src/pages/openai/page.tsx", "src/pages/api/upload.js", "src/pages/cheerio/page.js", "src/pages/vapi/get-called/page.js", "src/pages/Endpoints/page.tsx"],
  "exclude": ["node_modules"]
}
